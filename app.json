{
  "expo": {
    "name": "canguard-mobile-app",
    "slug": "canguard-mobile-app",
    "version": "1.0.0",
    "scheme": "canguard-mobile-app",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/335cca58-fa88-448a-9f2b-79bc1021c264"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hashi7412.canguardmobileapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "335cca58-fa88-448a-9f2b-79bc1021c264"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  },
  "plugins": [
    "expo-router",
    [
      "expo-build-properties",
      {
        "ios": {
          "useFrameworks": "static"
        }
      }
    ],
    [
      "expo-document-picker",
      {
        "iCloudContainerEnvironment": "Production"
      }
    ],
    [
      "expo-asset",
      {
        "assets": ["./src/assets/**/*"]
      }
    ]
  ],
  "experiments": {
    "typedRoutes": true
  },
  "extra": {
    "router": {
      "origin": false
    },
    "eas": {
      "projectId": "d867c814-8d30-4ce9-87f2-95483398c8e6"
    }
  }
}
